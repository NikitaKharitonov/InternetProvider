<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="300.0" spacing="8.0" xmlns="http://javafx.com/javafx/10.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controllers.CreateInternetScene">
    <padding>
        <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
    </padding>
   <children>
      <Label text="Name: " />
      <TextField fx:id="nameField" />
      <Label text="Speed: " />
      <Spinner fx:id="speedSpinner" />
      <Label text="Connection type: " />
      <ChoiceBox fx:id="connectionTypeChoiceBox" prefWidth="150.0" />
      <Label text="Antivirus: " />
      <CheckBox fx:id="antivirusCheckBox" mnemonicParsing="false" text="Antivirus" />
      <Label text="Activation date: " />
      <DatePicker fx:id="activationDatePicker" />
      <Button mnemonicParsing="false" onAction="#createButtonClicked" text="Create" />
      <Button mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
   </children>

</VBox>
